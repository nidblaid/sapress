<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--    <function name="compute_forward_button" model="soch.transport.order"/>-->
    <data>

        <record id="seq_customer_request" model="ir.sequence">
            <field name="name">customer_request_sequencer</field>
            <field name="code">sochepress.customer.request</field>
            <field name="prefix">DM%(y)s%(month)s</field>
            <field name="padding">5</field>
            <field name="implementation">standard</field>
        </record>
        <record id="seq_request_removal_order" model="ir.sequence">
            <field name="name">request_removal_order.sequencer</field>
            <field name="code">customer.request.removal.order</field>
            <field name="prefix">BE%(month)s%(year)s</field>
            <field name="padding">6</field>
            <field name="implementation">standard</field>
        </record>

        <record id="seq_customer_request_line" model="ir.sequence">
            <field name="name">customer_request_sequencer_line</field>
            <field name="code">sochepress.customer.request.line</field>
            <field name="prefix"></field>
            <field name="padding">13</field>
            <field name="implementation">standard</field>
        </record>
        <record id="sochepress_contract" model="ir.sequence">
            <field name="name">sochepress_contract_sequencer</field>
            <field name="code">sochepress.contract</field>
            <field name="prefix">CTR</field>
            <field name="padding">3</field>
            <field name="implementation">standard</field>
        </record>
        <record id="sochepress_transports" model="ir.sequence">
            <field name="name">sochepress_transports_sequencer</field>
            <field name="code">soch.transport.order</field>
            <field name="prefix">OT</field>
            <field name="padding">4</field>
            <field name="implementation">standard</field>
        </record>


        <record id="sochepress_transports_otr" model="ir.sequence">
            <field name="name">sochepress_transports_sequencer</field>
            <field name="code">transport_order_collect</field>
            <field name="prefix">OTR</field>
            <field name="padding">4</field>
        </record>

        <record id="sochepress_transports_ott" model="ir.sequence">
            <field name="name">sochepress_transports_sequencer</field>
            <field name="code">transport_order_primary</field>
            <field name="prefix">OTT</field>
            <field name="padding">4</field>
        </record>

        <record id="sochepress_transports_otl" model="ir.sequence">
            <field name="name">sochepress_transports_sequencer</field>
            <field name="code">transport_order_delivery</field>
            <field name="prefix">OTL</field>
            <field name="padding">4</field>
        </record>
    </data>
</odoo>
