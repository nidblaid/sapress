<odoo>
    <!--tree-->
    <record id="view_sls_return_method_tree" model="ir.ui.view">
        <field name="name">sls.return.method.tree</field>
        <field name="model">sls.return.method</field>
        <field name="arch" type="xml">
            <tree default_order="create_date desc">
                <field name="active" invisible="1"/>
                <field name="sequence" widget="handle"/>
                <field name="name"/>
                <field name="code"/>
                <field name="create_date" invisible="1"/>
            </tree>
        </field>
    </record>
    <!--search-->

    <!--form-->
    <record id="view_sls_return_method_form" model="ir.ui.view">
        <field name="name">sls.return.method.form</field>
        <field name="model">sls.return.method</field>
        <field name="arch" type="xml">
            <form string="Payment multi company">
                <sheet>
                    <group>
                        <field name="name" required="1"/>
                        <field name="sequence"/>
                        <field name="code"/>
                        <field name="active" invisible="1"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <!--action-->

    <record id="sls_return_method_action" model="ir.actions.act_window">
        <field name="name">Sls return method</field>
        <field name="res_model">sls.return.method</field>
        <field name="view_mode">tree,form</field>
    </record>


</odoo>
